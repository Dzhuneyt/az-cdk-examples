{
  "name": "public-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_USER_POOL_ID=$USER_POOL_ID REACT_APP_USER_POOL_CLIENT_ID=$USER_POOL_CLIENT_ID REACT_APP_USER_POOL_DOMAIN_PREFIX=$USER_POOL_DOMAIN_PREFIX HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .eslintignore . --ext ts --ext tsx",
    "lint:fix": "yarn lint --fix",
    "build:lib": "rm -rf dist && rollup --config rollup.config-rlib.js",
    "eject": "react-scripts eject",
    "fix:rcomps": "rm -rf ./src/rcomps && cp -r ./node_modules/@cpmech/rcomps ./src",
    "fix:gate": "rm -rf ./src/gate && cp -r ./node_modules/@cpmech/gate ./src",
    "postinstall": "npm run fix:rcomps && npm run fix:gate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cpmech/gate": "^0.3.0",
    "@cpmech/rcomps": "^1.15.0",
    "@cpmech/react-icons": "^2.7.0",
    "@emotion/core": "^10.0.22",
    "@reach/router": "^1.2.1",
    "aws-amplify": "^1.2.4",
    "aws-amplify-react": "^2.5.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-responsive": "^8.0.1",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-facebook-login": "^4.1.0",
    "@types/react-responsive": "^8.0.2",
    "@types/reach__router": "^1.2.6",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  }
}
