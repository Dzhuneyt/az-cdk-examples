  "repository": "cpmech/az-cdk",
  "scripts": {
    "new:tester": "ts-node -O '{\"module\":\"commonjs\",\"resolveJsonModule\":true}' scripts/createTesterUser.ts --resolveJsonModule",
    "preinstall": "cd layers/nodejs/ && npm install --no-bin-links",
    "cdk:simple": "bash ./scripts/cdk.bash",
    "cdk": "npm run build && bash ./scripts/cdk.bash",
    "build:api": "rm -rf dist_api && rollup --config rollup.config-api.js",
    "build:cognito": "rm -rf dist_cognito && rollup --config rollup.config-cognito.js",
    "build:graphql": "rm -rf dist_graphql && rollup --config rollup.config-graphql.js",
    "build": "npm run --silent build:api && npm run --silent build:cognito && npm run --silent build:graphql",
    "test": "rm -rf cdk.out && jest --verbose",
    "tw": "jest --watch --verbose",
    "cov": "npm run build && jest --coverage",
    "integ": "jest -c jest.config-integ.js --verbose --runInBand",
    "integw": "jest -c jest.config-integ.js --verbose --runInBand --watch"
  },
  "description": "examples",
  "keywords": [
    "amazon",
    "aws",
    "cdk",
    "cloudformation"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.23",
