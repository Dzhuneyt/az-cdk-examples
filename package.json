    "aws-amplify": "^2.2.0",
    "axios": "^0.19.0",
    "graphql": "file:./layers/nodejs/node_modules/graphql",
    "graphql-request": "^1.8.2",
    "graphql-tag": "file:./layers/nodejs/node_modules/graphql-tag",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "mailparser": "file:./layers/nodejs/node_modules/mailparser",
    "prettier": "^1.19.1",
    "rollup": "^1.27.13",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3",
